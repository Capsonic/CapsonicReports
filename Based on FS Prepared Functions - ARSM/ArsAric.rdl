,T2.UnappliedCreditBalanceLocalAmount UnappliedCreditBalanceAmt


,T3.TermsNetDueDate
,T3.InvoiceNumber
,T3.InvoiceType
,T3.InvoiceStatus

,case
	when T3.InvoiceType = 'R' then -T3.TotalInvoiceLocalAmount
	else T3.TotalInvoiceLocalAmount
 end as TotalInvoiceAmt
,case
	when T3.InvoiceType = 'R' then -T3.DiscountTakenLocalAmount
	else T3.DiscountTakenLocalAmount
 end as DiscountTakenAmt
,case
	when T3.InvoiceType = 'R' then -T3.PaymentLocalAmount
	else T3.PaymentLocalAmount
 end as PaymentAmt

,case 
 when datediff(day,T3.TermsNetDueDate,T3.InvoicePaidInFullDate) &lt; 0
 then 0
 else datediff(day,T3.TermsNetDueDate,T3.InvoicePaidInFullDate)
 end  DaysPastDue


from dbo.FS_Customer T1 (READUNCOMMITTED) 
left join dbo.FS_CustomerData T2  (READUNCOMMITTED) on (T1.CustomerKey = T2.CustomerKey)
left join dbo.FS_ARInvoiceHeader T3  (READUNCOMMITTED) on (T1.CustomerKey = T3.CustomerKey)
left join dbo.FS_CurrencyCode T4 (READUNCOMMITTED) on T1.CustomerCurrencyCode = T4.CurrencyCode

where T1.CustomerID = @CustomerID 
