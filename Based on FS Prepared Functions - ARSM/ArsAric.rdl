        <CommandText>----------------------------------------------------------------------------------------------------------
-- NAME:	ARIC
-- COPYRIGHT: Copyright 2004, SoftBrands Corporation. All rights reserved.
-- REVISION:  $Revision: $
--
-- FUNCTION:  Used in ARIC      Show the A/R Invoices By Customer;
--                   represent the user of FourthShift can revise the currency
--	        from Local 'L' to Foreign 'F', vice versa.
--	        When @InvoiceOption = 'O'  , the query must make the InvoiceStatus &lt;&gt;'Y'
--	        When @InvoiceOption = 'C'  , the query must make the InvoiceStatus ='Y'
-------------------------------------------------------------------------------------------------------------

if @CurrencyCode = 'L'  and @InvoiceOption = 'O'  

select 
T1.CustomerID
,T1.CustomerName
,T1.AccountingContact
,T1.AccountingContactPhone


,T1.CustomerControllingCode
,T4.DecimalPrecision CustDecimalPrecision
,T4.CurrencySymbol CustCurrencySymbol
,T4.CurrencySymbolPlacement CustCurrencySymbolPlacement
,T4.IsEECMember CustIsEECMember
,T4.CurrencyCode CustCurrencyCode
,T3.InvoiceExchangeRateOperator1
,T3.InvoiceExchangeRate1
,T3.InvoiceExchangeRateOperator2
,T3.InvoiceExchangeRate2

,T2.ARPastDueLocalAmount3 ARPastDueAmt3
,T2.ARPastDueLocalAmount2 ARPastDueAmt2
,T2.ARPastDueLocalAmount1 ARPastDueAmt1
,T2.ARCurrentBalanceLocalAmount ARCurrentBalanceAmt
,T2.OnHoldBalanceLocalAmount OnHoldBalanceAmt
,T2.ARBalanceDueLocalAmount ARBalanceDueAmt



,T2.UnappliedCashBalanceLocalAmount UnappliedCashBalanceAmt
,T2.UnappliedCreditBalanceLocalAmount UnappliedCreditBalanceAmt


,T3.TermsNetDueDate
,T3.InvoiceNumber
,T3.InvoiceType
,T3.InvoiceStatus

,case
	when T3.InvoiceType = 'R' then -T3.TotalInvoiceLocalAmount
	else T3.TotalInvoiceLocalAmount
 end as TotalInvoiceAmt
,case
	when T3.InvoiceType = 'R' then -T3.DiscountTakenLocalAmount
	else T3.DiscountTakenLocalAmount
 end as DiscountTakenAmt
,case
	when T3.InvoiceType = 'R' then -T3.PaymentLocalAmount
	else T3.PaymentLocalAmount
 end as PaymentAmt
,case 
 when datediff(day,T3.TermsNetDueDate,T3.InvoicePaidInFullDate) &lt; 0
 then 0
 else datediff(day,T3.TermsNetDueDate,T3.InvoicePaidInFullDate)
 end  DaysPastDue


from dbo.FS_Customer T1 (READUNCOMMITTED) 
left join dbo.FS_CustomerData T2  (READUNCOMMITTED) on (T1.CustomerKey = T2.CustomerKey)
left join dbo.FS_ARInvoiceHeader T3  (READUNCOMMITTED) on (T1.CustomerKey = T3.CustomerKey)
left join dbo.FS_CurrencyCode T4 (READUNCOMMITTED) on T1.CustomerCurrencyCode = T4.CurrencyCode

where T1.CustomerID = @CustomerID 
and  T3.InvoiceStatus &lt;&gt;'Y'
and T3.TermsNetDueDate&gt;=@FromDueDate
and T3.TermsNetDueDate &lt;=@ToDueDate
and len(rtrim(ltrim(T3.InvoiceNumber)))&gt;0   -- Invoice Number must exist
order by T3.TermsNetDueDate,T3.InvoiceNumber


if @CurrencyCode = 'L'  and @InvoiceOption = 'C'  

select 
T1.CustomerID
,T1.CustomerName
,T1.AccountingContact
,T1.AccountingContactPhone

,T1.CustomerControllingCode
,T4.DecimalPrecision CustDecimalPrecision
,T4.CurrencySymbol CustCurrencySymbol
,T4.CurrencySymbolPlacement CustCurrencySymbolPlacement
,T4.IsEECMember CustIsEECMember
,T4.CurrencyCode CustCurrencyCode
,T3.InvoiceExchangeRateOperator1
