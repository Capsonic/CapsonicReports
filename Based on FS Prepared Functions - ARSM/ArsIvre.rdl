        <CommandText>-----------------------------------------------------------------------------------------------------------

-- NAME:   IVRE

-- COPYRIGHT: Copyright 2004, SoftBrands Corporation. All rights reserved.

-- REVISION:  $Revision: $


--

-- FUNCTION:  Display is different for VAT and Sales Tax.

--            Display is different for different term code (D, M, S)

--

-------------------------------------------------------------------------------------------------------------


select
    T1.RIVREFNM   as   RecurringInvoiceReferenceNumber
   ,T1.RIVCUSID   as   CustomerID
   ,T1.RIVCURCD   as   RecurringInvoiceCurrencyCode
   ,T1.RIVREVDT   as   RecurringInvoiceLastMaintainedDate
   ,T1.RIVSTAT    as   RecurringInvoiceStatus
   ,T1.RIVSTART   as   StartDate
   ,T1.RIVEND     as   EndDate
   ,T1.RIVDAY     as   CreatedDay
   ,T1.RIVCNTCR   as   RecurringInvoiceControllingCurrency
   ,T1.RIVEXOP    as   RecurringInvoiceExchangeRateOperator1
   ,T1.RIVEXRT    as   RecurringInvoiceExchangeRate1
   ,T1.RIVEXOP9   as   RecurringInvoiceExchangeRateOperator2
   ,T1.RIVEXRT9   as   RecurringInvoiceExchangeRate2

   -- bill to info
   ,T2.CMB2NAME   as   BillToName
   ,T2.CMB2AD1    as   BillToAddress1
   ,T2.CMB2AD2    as   BillToAddress2
   ,T2.CMB2CITY   as   BillToCity
   ,T2.CMB2STAT   as   BillToState
   ,T2.CMB2ZIP    as   BillToZip
   ,T2.CMB2CNTR   as   BillToCountry
   ,T2.CMFOB      as   FOBPoint

   -- ship to info
   ,T1.RIVNAME    as   ShipToDeliveryLocationName
   ,T1.RIVAD1     as   ShipToDeliveryLocationAddress1
   ,T1.RIVAD2     as   ShipToDeliveryLocationAddress2
   ,T1.RIVCITY    as   ShipToDeliveryLocationCity
   ,T1.RIVSTATE   as   ShipToDeliveryLocationState
   ,T1.RIVZIP     as   ShipToDeliveryLocationZip
   ,T1.RIVCNTR    as   ShipToDeliveryLocationCountry
   ,T1.RIVSHVIA   as   CarrierName

   -- VCC code (only for VAT)
   ,T1.RIVVTCUS   as   VATCustomerCode

   -- pay terms
   ,T1.RIVPYHOW   as   TermsCode
   ,T1.RIVTRPC1   as   TermsDiscountPercent1
   ,T1.RIVTRDA1   as   TermsDiscountDays1
   ,T1.RIVTRPC2   as   TermsDiscountPercent2
   ,T1.RIVTRDA2   as   TermsDiscountDays2
   ,T1.RIVTRPC3   as   TermsDiscountPercent3
   ,T1.RIVTRDA3   as   TermsDiscountDays3
   ,T1.RIVTRPC4   as   TermsDiscountPercent4
   ,T1.RIVTRDA4   as   TermsDiscountDays4
   ,T1.RIVTRNET   as   TermsDiscountNetDays
   ,T1.RIVPYWEN   as   TermsMonthlyPaymentDay
   ,T1.RIVCTOFF   as   TermsMonthlyCutOffDay
   ,T1.RIVDELAY   as   TermsMonthlyDelay

   ,T1.RIVLNTOT   as   TotalLinesControllingAmount
   ,isnull(T5.TotalTaxControllingAmount,0) TotalTaxControllingAmount
   ,T1.RIVTOTAM   as   TotalRecurringInvoiceControllingAmount

   ,T1.RIVCOMCD   as   CommissionCode
   ,T1.RIVARACT   as   AccountsReceivableAccount

   -- line detail
   ,T6.RIDLINE    as   RecurringInvoiceLineNumber
   ,T6.RIDITEM    as   RecurringLineItemNumber
   ,T6.RIDACCTN   as   RecurringInvoiceLineSalesAccount
   ,T6.RIDQTYSH   as   RecurringInvoiceShipQuantity
   ,T6.RIDUNTPR   as   RecurringInvoiceControllingUnitPrice
   ,T6.RIDEXEMP   as   IsFederalPrimaryTaxExempt
   ,T6.RIDEXEMP$2 as   IsStateSecondaryTaxExempt
   ,T6.RIDEXEMP$3 as   IsCountyTaxExempt      -- (only for Sales Tax)
   ,T6.RIDEXEMP$4 as   IsMunicipalTaxExempt   -- (only for Sales Tax)
   ,T6.RIDCOMCD   as   RecurringInvoiceCommissionCode

---------------------------------
   ,T2.CMCNTCUR    as   CustomerControllingCode
   ,T2.CMCURCOD   as   CustCurrencyCode
   ,T7.CURRSYM  as   CustCurrencySymbol
   ,T7.CURRPRE       as   CustDecimalPrecision
   ,T7.CURRDESC    as   CustCurrencyCodeDescription
   ,T7.CURRFORM   as CustCurrencySymbolPlacement
   ,T7.CURREMU      as   CustIsEECMember
   ,T1.RIVTRFLG   as   TriFlag
   ,T8.IMDECPRE   as DecimalPrecision

from dbo.Fin_RIVMAST T1 (READUNCOMMITTED)
left join dbo.Mfg_CUSTMAST T2 (READUNCOMMITTED) on (T1.RIVCUSID = T2.CMCUSTID)
left join (select sum(T4.RISTAXRV) as TotalTaxControllingAmount, T3._Row as ARRecurringInvoiceHeaderKey
           from dbo.Fin_RIVMAST T3
           inner join dbo.Fin_RIVTAX T4 on T4._RITAXDTL_OwnRow = T3._Row
           group by T3._Row) T5 on T5.ARRecurringInvoiceHeaderKey = T1._Row
left join dbo.Fin_RIVDTL T6 (READUNCOMMITTED) on (T1._Row = T6._RIMSTDTL_OwnRow)
left join Mfg_CURRDEF T7 (READUNCOMMITTED) on (T2.CMCURCOD = T7.CURRCODE)
left join Mfg_ITMMAST T8 (READUNCOMMITTED) on(T6.RIDITEM = T8.IMPN)
where T1.RIVREFNM  = @RecRefNo</CommandText>
        <QueryParameters>
          <QueryParameter Name="@RecRefNo">
            <Value>=Parameters!RecRefNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
