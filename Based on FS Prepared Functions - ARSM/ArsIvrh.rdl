        <CommandText>-----------------------------------------------------------------------------------------------------------

-- NAME:   IVRH

-- COPYRIGHT: Copyright 2004, SoftBrands Corporation. All rights reserved.

-- REVISION:  $Revision: $

--

-- FUNCTION:  Display is different for VAT and Sales Tax.

--            Display is different for different term code (D, M, S)

--

-------------------------------------------------------------------------------------------------------------


select
    T1.RIVREFNM   as   RecurringInvoiceReferenceNumber
   ,T1.RIVCUSID   as   CustomerID
   ,T2.CMCUNAME   as   CustomerName

   -- Recurring Invoice section
   ,T1.RIVCURCD   as   RecurringInvoiceCurrencyCode
   ,T1.RIVSTAT    as   RecurringInvoiceStatus
   ,T1.RIVSTART   as   StartDate
   ,T1.RIVEND     as   EndDate
   ,T1.RIVDAY     as   CreatedDay
   ,T1.RIVCNTCR   as   RecurringInvoiceControllingCurrency
   ,T1.RIVEXOP    as   RecurringInvoiceExchangeRateOperator1
   ,T1.RIVEXRT    as   RecurringInvoiceExchangeRate1
   ,T1.RIVEXOP9   as   RecurringInvoiceExchangeRateOperator2
   ,T1.RIVEXRT9   as   RecurringInvoiceExchangeRate2

   
   -- Invoice Information section   
   ,T4.IVPAYHOW   as   InvoiceTermsCode
   ,convert(float,T4.IVTRMPC1)/100   as   TermsDiscountPercent1
   ,T4.IVTRMDA1   as   TermsDiscountDays1
   ,convert(float,T4.IVTRMPC2)/100   as   TermsDiscountPercent2
   ,T4.IVTRMDA2   as   TermsDiscountDays2
   ,convert(float,T4.IVTRMPC3)/100   as   TermsDiscountPercent3
   ,T4.IVTRMDA3   as   TermsDiscountDays3
   ,convert(float,T4.IVTRMPC4)/100   as   TermsDiscountPercent4
   ,T4.IVTRMDA4   as   TermsDiscountDays4
   ,T4.IVTRMNET   as   TermsDiscountNetDays
   ,T4.IVPAYWEN   as   TermsMonthlyPaymentDay
   ,T4.IVCUTOFF   as   TermsMonthlyCutOffDay
   ,T4.IVDELAY    as   TermsMonthlyDelay
   ,T4.IVPAYDY1   as   TermsSpecificPaymentDate1
   ,T4.IVPAYAM1   as   TermsSpecificPaymentAmount1
   ,T4.IVPAYDY2   as   TermsSpecificPaymentDate2
   ,T4.IVPAYAM2   as   TermsSpecificPaymentAmount2
   ,T4.IVPAYDY3   as   TermsSpecificPaymentDate3
   ,T4.IVPAYAM3   as   TermsSpecificPaymentAmount3
   ,T4.ARIMNTDT   as   TermsNetDueDate
   ,T4.IVPAYAM4   as   TermsSpecificNetDueAmount

   ,TotalTaxControllingAmount = case 
                                   when T1.RIVCNTCR = 'L' then T5.TotalTaxLocalAmount
                                   else T5.TotalTaxForeignAmount
                                end
   ,FederalTaxCode   = (select TaxCode 
                        from dbo.FS_ARInvoiceTax
                        where TaxAuthority = 1 and ARInvoiceHeaderKey = T4._Row)
   ,StateTaxCode     = (select TaxCode 
                        from dbo.FS_ARInvoiceTax
                        where TaxAuthority = 2 and ARInvoiceHeaderKey = T4._Row)
   ,CountyTaxCode    = (select TaxCode 
                        from dbo.FS_ARInvoiceTax
                        where TaxAuthority = 3 and ARInvoiceHeaderKey = T4._Row)
   ,MunicipalTaxCode = (select TaxCode 
                        from dbo.FS_ARInvoiceTax
                        where TaxAuthority = 4 and ARInvoiceHeaderKey = T4._Row)

   ,T4.IVCOMCD    as   InvoiceCommissionCode
   ,T4.IVCNTCUR   as   InvoiceControllingCurrency
   ,T4.IVEXOP     as   InvoiceExchangeRateOperator1
   ,T4.IVEXRATE   as   InvoiceExchangeRate1
   ,T4.IVEXOP9    as   InvoiceExchangeRateOperator2
   ,T4.IVEXRAT9   as   InvoiceExchangeRate2

   -- Line History Info
   ,case 
    when T9.DateNumberFormat = 'C' or T9.DateNumberFormat = 'S'  -- these kind of format, need YY-MM accouting period
    then convert(varchar,T3.ARHSYEAR)+ '-' + case T3.ARHSMTH 
                                        when 1 then '01'
                                        when 2 then '02'
                                        when 3 then '03'
                                        when 4 then '04'
                                        when 5 then '05'
                                        when 6 then '06'
                                        when 7 then '07'
                                        when 8 then '08'
                                        when 9 then '09'
                                        else convert(varchar,T3.ARHSMTH)
                                        end
    else case   T3.ARHSMTH -- these kind of format, need MM-YY accouting period
         when 1 then '01'
         when 2 then '02'
         when 3 then '03'
         when 4 then '04'
         when 5 then '05'
         when 6 then '06'
         when 7 then '07'
         when 8 then '08'
         when 9 then '09'
         else convert(varchar,T3.ARHSMTH)
         end  + '-'+convert(varchar,T3.ARHSYEAR)
    end CalendarPeriod
   ,T3.ARHSMTH    as   CalendarMonth
   ,T3.ARHSYEAR   as   CalendarYear
   ,T3.ARHSLAST   as   LastActionUserID
   ,T3.ARHSIVNO   as   InvoiceNumber
   ,T3.ARHSDATE   as   SystemInvoiceDate
   ,T3.ARHSAMT    as   TotalInvoiceControllingAmount
   ,InvoiceHistoryStatus = case T3.ARHSST
                              when 'N' then
                                 case T4.IVSTAT
                                    when 'A' then 'P'--'PRE-PRINTED INVOICE CREATED'
                                    when 'U' then 'P'--'PRE-PRINTED INVOICE CREATED'
                                    when 'N' then 'N'--'NO PAID'
                                    when 'P' then 'PP'--'PARTIALLY PAID'
                                    when 'Y' then 'F'--'FULLY PAID'
                                    when 'H' then 'IH'--'INVOICE ON HOLD'
                                    when 'X' then 'IC'--'INVOICE CANCELED'
                                 end
                              when 'D' then 'ID'--'INVOICE DELETED'
                              when 'P' then 'IP'--'INVOICE PURGED'
                           end
   

-----------------------------
   ,T4.IVCO
   ,T4.IVCOSEQ
   ,T4.IVTRIFLG TriFlag
   ,T8.CURRCODE   as   CustCurrencyCode
   ,T8.CURRSYM  as   CustCurrencySymbol
   ,T8.CURRFORM   as CustCurrencySymbolPlacement
   ,T8.CURRPRE       as   CustDecimalPrecision
   ,T8.CURRDESC    as   CustCurrencyCodeDescription
   ,T8.CURREMU      as   CustIsEECMember

from dbo.Fin_RIVMAST T1 (readuncommitted)
left join dbo.Mfg_CUSTMAST T2 (readuncommitted) on (T1.RIVCUSID = T2.CMCUSTID)
left join dbo.Fin_RIVHIST T3 (readuncommitted) on (T1._Row = T3._IVRCRHIS_OwnRow)
left join dbo.Fin_IVMAST T4 (readuncommitted) on (T3._Row = T4._IVMSTHIS_OwnRow)
left join (select sum(T7.ISTAXRCV) as TotalTaxLocalAmount
           ,sum(T7.ISFTAXRV)   as TotalTaxForeignAmount
           ,T6._Row as ARInvoiceHeaderKey
           from dbo.Fin_IVMAST T6
           inner join dbo.Fin_IVTAX T7 on T7._IVTAXDTL_OwnRow = T6._Row
           group by T6._Row) T5 on T5.ARInvoiceHeaderKey = T4._Row
left join Mfg_CURRDEF T8 (readuncommitted) on (T2.CMCURCOD = T8.CURRCODE)
cross join FS_SystemConfig T9 (readuncommitted)
where   T1.RIVREFNM   =  @RefNo
order by CalendarYear, CalendarMonth</CommandText>
        <QueryParameters>
          <QueryParameter Name="@RefNo">
            <Value>=Parameters!RefNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
