      <Query>
        <DataSourceName>FSDB</DataSourceName>
        <CommandText>----------------------------------------------------------------------------------
-- NAME:	IVRV
-- COPYRIGHT: Copyright 2004, SoftBrands Corporation. All rights reserved.
-- REVISION:  $Revision: $
--
-- FUNCTION:  Use IVRV query to show the Invoice Review
--                   We get info from View FS_ARInvoiceHeader
--	        when InvoiceType ='R', we make TotalLinesAmount;
--	       TotalTaxAmount;TotalInvoiceAmount  nagtive .
--	       
--	        
--	        
----------------------------------------------------------------------------------







select distinct
T1.CustomerID
,T1.CONumber
,T1.COSequenceNumber
,T1.InvoiceType
,case 
	when T1.ShippedDate = '1980-01-01' then null
	else T1.ShippedDate
 end as ShippedDate

,case 
	when T1.InvoiceType ='R' then -T1.TotalLinesLocalAmount
	else T1.TotalLinesLocalAmount
 end as TotalLinesAmount
,case 
	when T1.InvoiceType ='R' then -T2.TotalTaxLocalAmount
	else T2.TotalTaxLocalAmount
 end as TotalTaxAmount
,case 
	when T1.InvoiceType ='R' then -T1.RoundingNonControllingAmount
	else T1.RoundingNonControllingAmount
 end as RoundingNonControllingAmount
,case 
	when T1.InvoiceType ='R' then -T1.TotalInvoiceLocalAmount
	else T1.TotalInvoiceLocalAmount
 end as TotalInvoiceAmount
,T1.InvoiceStatus
,T1.InvoiceControllingCode



from FS_ARInvoiceHeader T1 (READUNCOMMITTED) 
left join (select ARInvoiceHeaderKey, sum(TotalTaxLocalAmount) TotalTaxLocalAmount from dbo.FS_ARInvoiceTax (READUNCOMMITTED) group by ARInvoiceHeaderKey) T2 on (T1.ARInvoiceHeaderKey = T2.ARInvoiceHeaderKey)
where (T1.InvoiceStatus = 'U'  or  T1.InvoiceStatus ='A' )
and T1.CustomerID&gt;=@FromCustID 
and T1.CustomerID &lt;=@ToCustID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@FromCustID">
            <Value>=Parameters!FromCustID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToCustID">
            <Value>=Parameters!ToCustID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="CHOOSEDB">
