          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>FSDB</DataSourceName>
        <CommandText>-----------------------------------------------------------------------------------------------------------
-- NAME:	CHOOSEDB
-- COPYRIGHT: Copyright 2004, SoftBrands Corporation. All rights reserved.
-- REVISION:  $Revision: $
--
-- FUNCTION:  Find Database type is V(VAT) or TAX
--                   
--	        
--	        
--	        
----------------------------------------------------------------------------------------------------------


select substring(CLBODY,72,1) as DatabaseType
from dbo.Mfg_PARAMETR
where CLTYPE = 'CR'
and CLID = 'INCF'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
