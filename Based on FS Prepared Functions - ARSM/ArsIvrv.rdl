        </Field>
        <Field Name="InvoiceStatus">
          <DataField>InvoiceStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InvoiceControllingCode">
          <DataField>InvoiceControllingCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
