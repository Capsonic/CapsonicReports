        <CommandText>----------------------------------------------------------------------------------------------------------
-- NAME:	ARPH
-- COPYRIGHT: Copyright 2004, SoftBrands Corporation. All rights reserved.
-- REVISION:  $Revision: $
--
-- FUNCTION:  Used in ARPH      Show the A/R Invoices payment history;
--                   CurrencyCode is the variable for the query that can change currency code
--	        from Local 'L' to Foreign 'F', vice versa.
--	        
--	        
-------------------------------------------------------------------------------------------------------------




if @CurrencyCode = 'L'


select 
T1.CustomerID 
,T1.CustomerName
,T1.AccountingContact
,T1.AccountingContactPhone

,T2.ARPastDueLocalAmount3 ARPastDueAmount3
,T2.ARPastDueLocalAmount2 ARPastDueAmount2
,T2.ARPastDueLocalAmount1 ARPastDueAmount1
,T2.ARCurrentBalanceLocalAmount ARCurrentBalanceAmount
,T2.OnHoldBalanceLocalAmount OnHoldBalanceAmount
,T2.ARBalanceDueLocalAmount ARBalanceDueAmount

,T2.UnappliedCashBalanceLocalAmount UnappliedCashBalanceAmount
,T2.UnappliedCreditBalanceLocalAmount UnappliedCreditBalanceAmount


,T3.CashReceiptForm
,T3.CashReceiptReferenceNumber
,T3.CashReceiptType

,T3.CashReceiptLocalAmount CashReceiptAmount
,T3.AppliedLocalAmount AppliedAmount
,T3.DiscountAllowedLocalAmount DiscountAllowedAmount
,case left(T3.CashReceiptReferenceNumber,1)
 when 'R'
 then T3.CashReceiptLocalAmount - T3.AppliedCreditMemoLocalAmount
 else T3.UnappliedLocalAmount
 end UnappliedAmount

,T3.CashReceiptDate



from dbo.FS_Customer T1 (READUNCOMMITTED) 
left join dbo.FS_CustomerData T2  (READUNCOMMITTED) on (T1.CustomerKey = T2.CustomerKey)
left join dbo.FS_ARCreditMemo T3  (READUNCOMMITTED) on (T1.CustomerKey = T3.CustomerKey)

where T1.CustomerID = @CustomerID 
and (T3.CashReceiptForm + T3.CashReceiptReferenceNumber) &gt;= @FromRef
and (T3.CashReceiptForm + T3.CashReceiptReferenceNumber) &lt;= @ToRef
and (T3.CashReceiptType = 30 or T3.CashReceiptType = 36 or T3.CashReceiptType = 40)




union all





select 
T1.CustomerID 
,T1.CustomerName
,T1.AccountingContact
,T1.AccountingContactPhone

,T2.ARPastDueLocalAmount3 ARPastDueAmount3
,T2.ARPastDueLocalAmount2 ARPastDueAmount2
,T2.ARPastDueLocalAmount1 ARPastDueAmount1
,T2.ARCurrentBalanceLocalAmount ARCurrentBalanceAmount
,T2.OnHoldBalanceLocalAmount OnHoldBalanceAmount
,T2.ARBalanceDueLocalAmount ARBalanceDueAmount

,T2.UnappliedCashBalanceLocalAmount UnappliedCashBalanceAmount
,T2.UnappliedCreditBalanceLocalAmount UnappliedCreditBalanceAmount


,T3.CashReceiptForm
,T3.CashReceiptReferenceNumber
,T3.CashReceiptType

,T3.CashReceiptLocalAmount CashReceiptAmount
,T3.AppliedLocalAmount AppliedAmount
,T3.DiscountAllowedLocalAmount DiscountAllowedAmount
,case left(T3.CashReceiptReferenceNumber,1)
 when 'R'
 then T3.CashReceiptLocalAmount - T3.AppliedCreditMemoLocalAmount
 else T3.UnappliedLocalAmount
 end UnappliedAmount

,T3.CashReceiptDate


from dbo.FS_Customer T1 (READUNCOMMITTED) 
left join dbo.FS_CustomerData T2  (READUNCOMMITTED) on (T1.CustomerKey = T2.CustomerKey)
left join dbo.FS_ARCashReceipt T3  (READUNCOMMITTED) on (T1.CustomerKey = T3.CustomerKey)

where T1.CustomerID = @CustomerID 
and (T3.CashReceiptForm + T3.CashReceiptReferenceNumber) &gt;= @FromRef
and (T3.CashReceiptForm + T3.CashReceiptReferenceNumber) &lt;= @ToRef
and (T3.CashReceiptType = 30 or T3.CashReceiptType = 36 or T3.CashReceiptType = 40)

order by T3.CashReceiptForm,T3.CashReceiptReferenceNumber



















if @CurrencyCode = 'F'

select 
T1.CustomerID 
,T1.CustomerName
,T1.AccountingContact
,T1.AccountingContactPhone

,T2.ARPastDueForeignAmount3 ARPastDueAmount3
,T2.ARPastDueForeignAmount2 ARPastDueAmount2
,T2.ARPastDueForeignAmount1 ARPastDueAmount1
,T2.ARCurrentBalanceForeignAmount ARCurrentBalanceAmount
,T2.OnHoldBalanceForeignAmount OnHoldBalanceAmount
,T2.ARBalanceDueForeignAmount ARBalanceDueAmount

,T2.UnappliedCashBalanceForeignAmount UnappliedCashBalanceAmount
,T2.UnappliedCreditBalanceForeignAmount UnappliedCreditBalanceAmount


,T3.CashReceiptForm
,T3.CashReceiptReferenceNumber
,T3.CashReceiptType

,T3.CashReceiptForeignAmount CashReceiptAmount
,T3.AppliedForeignAmount AppliedAmount
,T3.DiscountAllowedForeignAmount DiscountAllowedAmount
,case left(T3.CashReceiptReferenceNumber,1)
 when 'R'
 then T3.CashReceiptForeignAmount - T3.AppliedCreditMemoForeignAmount
 else T3.UnappliedForeignAmount
 end UnappliedAmount

,T3.CashReceiptDate


from dbo.FS_Customer T1 (READUNCOMMITTED) 
left join dbo.FS_CustomerData T2  (READUNCOMMITTED) on (T1.CustomerKey = T2.CustomerKey)
left join dbo.FS_ARCreditMemo T3  (READUNCOMMITTED) on (T1.CustomerKey = T3.CustomerKey)

where T1.CustomerID = @CustomerID 
and (T3.CashReceiptForm + T3.CashReceiptReferenceNumber) &gt;= @FromRef
and (T3.CashReceiptForm + T3.CashReceiptReferenceNumber) &lt;= @ToRef
and (T3.CashReceiptType = 30 or T3.CashReceiptType = 36 or T3.CashReceiptType = 40)





union all





select 
T1.CustomerID 
,T1.CustomerName
,T1.AccountingContact
,T1.AccountingContactPhone

,T2.ARPastDueForeignAmount3 ARPastDueAmount3
,T2.ARPastDueForeignAmount2 ARPastDueAmount2
,T2.ARPastDueForeignAmount1 ARPastDueAmount1
,T2.ARCurrentBalanceForeignAmount ARCurrentBalanceAmount
,T2.OnHoldBalanceForeignAmount OnHoldBalanceAmount
,T2.ARBalanceDueForeignAmount ARBalanceDueAmount

,T2.UnappliedCashBalanceForeignAmount UnappliedCashBalanceAmount
,T2.UnappliedCreditBalanceForeignAmount UnappliedCreditBalanceAmount


,T3.CashReceiptForm
,T3.CashReceiptReferenceNumber
,T3.CashReceiptType

,T3.CashReceiptForeignAmount CashReceiptAmount
,T3.AppliedForeignAmount AppliedAmount
,T3.DiscountAllowedForeignAmount DiscountAllowedAmount
,case left(T3.CashReceiptReferenceNumber,1)
 when 'R'
 then T3.CashReceiptForeignAmount - T3.AppliedCreditMemoForeignAmount
 else T3.UnappliedForeignAmount
 end UnappliedAmount

,T3.CashReceiptDate


from dbo.FS_Customer T1 (READUNCOMMITTED) 
left join dbo.FS_CustomerData T2  (READUNCOMMITTED) on (T1.CustomerKey = T2.CustomerKey)
left join dbo.FS_ARCashReceipt T3  (READUNCOMMITTED) on (T1.CustomerKey = T3.CustomerKey)

where T1.CustomerID = @CustomerID 
and (T3.CashReceiptForm + T3.CashReceiptReferenceNumber) &gt;= @FromRef
and (T3.CashReceiptForm + T3.CashReceiptReferenceNumber) &lt;= @ToRef
and (T3.CashReceiptType = 30 or T3.CashReceiptType = 36 or T3.CashReceiptType = 40)

order by T3.CashReceiptForm,T3.CashReceiptReferenceNumber</CommandText>
