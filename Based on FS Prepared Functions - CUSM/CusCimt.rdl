        <CommandText>----------------------------------------------------------------------------------
-- NAME:	CIMT
-- COPYRIGHT: Copyright 2004, SoftBrands Corporation. All rights reserved.
-- REVISION:  $Revision: $
--
-- FUNCTION:  Used in CIMT    Date query from FS_Customer;FS_CustomerItem;FS_Item
--
--

----------------------------------------------------------------------------------








select 
cust.CustomerID
,cust.CustomerName
,cust.CustomerContact
,cust.CustomerContactPhone
,cuit.CustomerItemNumber
,cuit.CustomerItemDescription
,cuit.CustomerItemRevision
,cuit.TransportationLeadTimeDays
,item.ItemNumber
,isnull(cuit.CustomerItemNumber,'No customer items for this customer') Message
from dbo.FS_Customer cust (READUNCOMMITTED) 
left join dbo.FS_CustomerItem cuit  (READUNCOMMITTED) on (cust.CustomerKey = cuit.CustomerKey)
left join  dbo.FS_Item item  (READUNCOMMITTED) on (cuit.ItemKey = item.ItemKey)

where cust.CustomerID&gt;=@FromCust
          and cust.CustomerID&lt;=@ToCust</CommandText>
        <QueryParameters>
          <QueryParameter Name="@FromCust">
            <Value>=Parameters!FromCust.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToCust">
            <Value>=Parameters!ToCust.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
