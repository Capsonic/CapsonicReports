      <Query>
        <DataSourceName>FSDBMR</DataSourceName>
        <CommandText>IF @MainGroupSelect = 'ALL'
SELECT DISTINCT 
                      PO_QtyReq.VendorName, PO_QtyReq.PONumber, PO_QtyReq.VendorID, PO_QtyReq.Buyer, PO_QtyReq.POLineNumber, 
                      PO_QtyReq.POLineSubType, PO_QtyReq.POLineStatus, PO_QtyReq.POLineType,PO_QtyReq.LineItemOrderedQuantity,  PO_QtyReq.ReceiptQuantity, PO_QtyReq.OrderBalance,
                      PO_QtyReq.RequiredDate, PO_QtyReq.ItemUnitCost,  PO_QtyReq.ItemNumber, PO_QtyReq.ItemDescription, PO_QtyReq.MakeBuyCode, 
                      PO_QtyReq.ItemReference1,  PO_QtyReq.ITEM_REF1_DESC,PO_QtyReq.GroupTechnologyCode, PO_QtyReq.BUSINESS_TEAM, PO_QtyReq.BT_DESCRIPTION, PO_QtyReq.ItemKey
                     , MAX(DISTINCT FS_ItemCost_1.ItemKey) AS CostItemKey,  PO_QtyReq.ItemUnitCost, 
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '0') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType0,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '0') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost AS Variance_0P,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '1') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType1,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '1') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost AS Variance_1P,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '4') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType4,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '4') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost AS Variance_4P,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '5') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType5,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '5') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost AS Variance_5P,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '6') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType6,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '6') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost AS Variance_6P
FROM         (SELECT     TOP (100) PERCENT FS_Vendor.VendorName, _CAP_Class_Ref.ITEM_REF1_DESC, FS_POHeader.PONumber, FS_POHeader.VendorID, FS_POHeader.Buyer, 
                      FS_POLine.POLineNumber, FS_POLine.POLineSubType, FS_POLine.LineItemOrderedQuantity, FS_POLine.POLineStatus, FS_POLine.POLineType, 
                      FS_POLine.ReceiptQuantity, FS_POLine.RequiredDate, FS_POLineData.ItemUnitCost, 
                      FS_POLine.LineItemOrderedQuantity - FS_POLine.ReceiptQuantity AS OrderBalance, FS_Item.ItemNumber, FS_Item.ItemDescription, FS_Item.MakeBuyCode, 
                      FS_Item.ItemReference1, FS_Item.GroupTechnologyCode, _CAP_Class_Ref.BUSINESS_TEAM, _CAP_Class_Ref.BT_DESCRIPTION, FS_Item.ItemKey
FROM         FS_POLineData INNER JOIN
                      FS_Vendor INNER JOIN
                      FS_POLine INNER JOIN
                      FS_POHeader ON FS_POLine.POHeaderKey = FS_POHeader.POHeaderKey ON FS_Vendor.VendorKey = FS_POHeader.VendorKey ON 
                      FS_POLineData.POLineKey = FS_POLine.POLineKey INNER JOIN
                      FS_Item ON FS_Item.ItemKey = FS_POLine.ItemKey INNER JOIN
                      _CAP_Class_Ref ON FS_Item.ItemReference1 = _CAP_Class_Ref.ITEM_REF1
WHERE     (FS_POLine.POLineSubType &lt;&gt; 'B') AND (FS_POLine.POLineStatus &lt; '5') AND (FS_POLine.POLineType = 'P') AND (FS_POLine.RequiredDate    BETWEEN @StartDate AND @EndDate)) AS PO_QtyReq INNER JOIN
                      FS_ItemCost AS FS_ItemCost_1 WITH (nolock) ON PO_QtyReq.ItemKey = FS_ItemCost_1.ItemKey
GROUP BY PO_QtyReq.VendorName, PO_QtyReq.ITEM_REF1_DESC, PO_QtyReq.PONumber, PO_QtyReq.VendorID, PO_QtyReq.Buyer, PO_QtyReq.POLineNumber, 
                      PO_QtyReq.POLineSubType, PO_QtyReq.LineItemOrderedQuantity, PO_QtyReq.POLineStatus, PO_QtyReq.POLineType, PO_QtyReq.ReceiptQuantity, 
                      PO_QtyReq.RequiredDate, PO_QtyReq.ItemUnitCost, PO_QtyReq.OrderBalance, PO_QtyReq.ItemNumber, PO_QtyReq.ItemDescription, PO_QtyReq.MakeBuyCode, 
                      PO_QtyReq.ItemReference1, PO_QtyReq.GroupTechnologyCode, PO_QtyReq.BUSINESS_TEAM, PO_QtyReq.BT_DESCRIPTION, PO_QtyReq.ItemKey



having
                       (   (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '0') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost &lt;&gt; 0.0000) OR
                       (   (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '1') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost &lt;&gt; 0.0000) OR
                        (  (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '4') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost &lt;&gt; 0.0000) OR
                         ( (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '5') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost&lt;&gt; 0.0000) OR
                      (    (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '6') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost &lt;&gt; 0.0000) 




 IF @MainGroupSelect = 'I'
                                                                                           SELECT DISTINCT 
                      PO_QtyReq.VendorName, PO_QtyReq.PONumber, PO_QtyReq.VendorID, PO_QtyReq.Buyer, PO_QtyReq.POLineNumber, 
                      PO_QtyReq.POLineSubType, PO_QtyReq.POLineStatus, PO_QtyReq.POLineType,PO_QtyReq.LineItemOrderedQuantity,  PO_QtyReq.ReceiptQuantity, PO_QtyReq.OrderBalance,
                      PO_QtyReq.RequiredDate, PO_QtyReq.ItemUnitCost,  PO_QtyReq.ItemNumber, PO_QtyReq.ItemDescription, PO_QtyReq.MakeBuyCode, 
                      PO_QtyReq.ItemReference1,  PO_QtyReq.ITEM_REF1_DESC,PO_QtyReq.GroupTechnologyCode, PO_QtyReq.BUSINESS_TEAM, PO_QtyReq.BT_DESCRIPTION, PO_QtyReq.ItemKey
                     , MAX(DISTINCT FS_ItemCost_1.ItemKey) AS CostItemKey,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '0') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType0,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '0') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost AS Variance_0P,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '1') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType1,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '1') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost AS Variance_1P,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '4') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType4,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '4') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost AS Variance_4P,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '5') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType5,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '5') AND (PO_QtyReq.ItemKey = ItemKey)) - PO_QtyReq.ItemUnitCost AS Variance_5P,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
                            WHERE      (CostType = '6') AND (PO_QtyReq.ItemKey = ItemKey)) AS CostType6,
                          (SELECT     TotalRolledCost
                            FROM          FS_ItemCost AS FS_ItemCost_1 WITH (nolock)
